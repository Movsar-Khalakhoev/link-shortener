  "bugs": {
    "url": "https://github.com/Movsar-Khalakhoev/link-shortener/issues"
  },
  "homepage": "https://github.com/Movsar-Khalakhoev/link-shortener#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "express-validator": "^6.9.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.9",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
